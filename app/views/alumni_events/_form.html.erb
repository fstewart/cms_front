<%= simple_form_for(@alumni_event) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :location %>
    <%= f.input :event_start %>
    <%= f.input :event_end %>
    <%= f.association :alumni_chapter %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
