<div class="nested-fields well well-compact">
  <h4 style="text-align:center;">Timeline Details</h4>
  <div class="col-md-6">
    <div class="form-inline" style="width: 100%;margin: 0 auto;">
    <p><%= f.input :event_date  %></p>
    <p><%= f.input :event_title  %></p>
    <p><%= f.input :event_content  %></p>
    <p><%= f.input :status, :label => 'Event Status'  %></p>

    <%= link_to_remove_association f, data: {:confirm => 'do you really want to do this?'}, class: 'btn btn-default btn-xs' do %>
        <p>Remove this event from the timeline.</p>
    <% end %>
    </div>
  </div>
  <div class="col-md-6">
    <div style="width: 50%;margin: 0 auto;">
      <h3>Example:</h3>

    </div>
  </div>

</div>