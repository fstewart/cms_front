<%= simple_form_for(@lift_course) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :lift_session %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :meeting_day %>
    <%= f.input :meeting_time %>
    <%= f.input :seats %>
    <%= f.association :lift_location %>
    <%= f.input :location_url %>
    <%= f.input :status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
