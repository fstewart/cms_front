<%= simple_form_for(@center) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :unit_type_id %>
    <%= f.input :name %>
    <%= f.input :slug %>
    <%= f.input :building_id %>
    <%= f.input :phone %>
    <%= f.input :fax %>
    <%= f.input :office %>
    <%= f.input :mailing_address %>
    <%= f.input :po_box %>
    <%= f.input :header_image %>
    <%= f.input :user_id %>
    <%= f.input :facebook %>
    <%= f.input :linkedin %>
    <%= f.input :twitter %>
    <%= f.input :instagram %>
    <%= f.input :snapchat %>
    <%= f.input :vimeo %>
    <%= f.input :youtube %>
    <%= f.input :contact_name %>
    <%= f.input :contact_email %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
