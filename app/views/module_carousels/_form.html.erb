<%= simple_form_for(@module_carousel) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :priority %>
    <%= f.input :title %>
    <%= f.input :desc %>
    <%= f.input :image, as: :file %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
