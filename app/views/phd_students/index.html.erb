<p id="notice"><%= notice %></p>

<h1>Listing Phd Students</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Bio</th>
      <th>Email</th>
      <th>Descriptor</th>
      <th>Honors</th>
      <th>Photo</th>
      <th>Phd</th>
      <th>Cv</th>
      <th>Jmpaper</th>
      <th>Status</th>
      <th>Pubfile</th>
      <th>Cvfile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phd_students.each do |phd_student| %>
      <tr>
        <td><%= phd_student.name %></td>
        <td><%= phd_student.bio %></td>
        <td><%= phd_student.email %></td>
        <td><%= phd_student.descriptor %></td>
        <td><%= phd_student.honors %></td>
        <td><%= phd_student.photo %></td>
        <td><%= phd_student.phd_id %></td>
        <td><%= phd_student.cv %></td>
        <td><%= phd_student.jmpaper %></td>
        <td><%= phd_student.status %></td>
        <td><%= phd_student.pubfile %></td>
        <td><%= phd_student.cvfile %></td>
        <td><%= link_to 'Show', phd_student %></td>
        <td><%= link_to 'Edit', edit_phd_student_path(phd_student) %></td>
        <td><%= link_to 'Destroy', phd_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Phd student', new_phd_student_path %>
