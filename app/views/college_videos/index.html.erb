<section class="site-content">
	<div class="container">
		<div class="row">
			
			<p id="notice"><%= notice %></p>
			
			<h1>Listing College Videos</h1>
			
			<table class="table table-striped">
			  <thead>
			    <tr>
			      <th>Name</th>
			      <th>Url</th>
			      <th colspan="3"></th>
			    </tr>
			  </thead>
			
			  <tbody>
			    <% @college_videos.each do |college_video| %>
			      <tr>
			        <td><%= college_video.name %></td>
			        <td><%= college_video.url %></td>
			        <td><%= link_to 'Show', college_video %></td>
			        <td><%= link_to 'Edit', edit_college_video_path(college_video) %></td>
			        <td><%= link_to 'Destroy', college_video, method: :delete, data: { confirm: 'Are you sure?' } %></td>
			      </tr>
			    <% end %>
			  </tbody>
			</table>
			
			<br>
			
			<%= link_to 'New College video', new_college_video_path %>

		</div> <!-- .col-md-3 -->
	</div> <!-- .row -->
</section> <!-- .container -->