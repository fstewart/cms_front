<%= simple_form_for(@alumni_chapter) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :address %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :members %>
    <%= f.input :active %>
    <%= f.input :alumni_event_id %>
    <%= f.input :contact %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
